{
  "name": "Video to GIF",
  "version": "1.0.0",
  "description": "Convert video to GIF, locally",
  "permissions": [],
  "content_security_policy": {
    "extension_pages": "default-src 'none'; script-src 'self' 'wasm-unsafe-eval'; img-src 'self' data:; style-src 'self';"
  },
  "action": {
    "default_icon": {
      "16": "assets/favicon.ico"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+Shift+G"
      }
    }
  },
  "icons": {
    "16": "assets/favicon.ico"
  },
  "manifest_version": 3
}
